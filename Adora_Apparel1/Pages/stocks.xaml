<UserControl x:Class="Adora_Apparel1.stocks"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Pages/StocklotsPurchasing.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Stock Purchasing" Source="/Pages/StocklotsPurchasing.xaml" />
                <mui:Link DisplayName="Stock Sales" Source="" />
                <mui:Link DisplayName="Monthly Sales Summary" Source="" />
                <mui:Link DisplayName="Purchasing Summary" Source="" />
                <mui:Link DisplayName="Sales Comparison" Source="" />
                <mui:Link DisplayName="Purchasing Comparison" Source="" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
